<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="forestPU" transaction-type="JTA">
    <jta-data-source>jdbc/forest</jta-data-source>
    <class>com.forest.entity.Administrator</class>
    <class>com.forest.entity.Category</class>
    <class>com.forest.entity.Customer</class>
    <class>com.forest.entity.CustomerOrder</class>
    <class>com.forest.entity.Groups</class>
    <class>com.forest.entity.OrderDetail</class>
    <class>com.forest.entity.OrderStatus</class>
    <class>com.forest.entity.Person</class>
    <class>com.forest.entity.Product</class>
    <shared-cache-mode>NONE</shared-cache-mode>
    <properties>
        </properties>
  </persistence-unit>
</persistence>
